{
    "family": "llmopstdlive",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::339712993834:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "8192",
    "containerDefinitions": [
      {
        "name": "llmops-live-container",
        "image": "339712993834.dkr.ecr.us-east-2.amazonaws.com/llmopsrepolive",
        "cpu": 1024,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "name": "llmops-live-container-8080-tcp",
            "appProtocol": "http"
          }
        ],
        "healthCheck": {
          "command": ["CMD-SHELL", "curl -f http://localhost:8080/health || exit 1"],
          "interval": 30,
          "timeout": 5,
          "retries": 3,
          "startPeriod": 60
        },
        "environment": [
          {
            "name": "ENV",
            "value": "production"
          },
          {
            "name": "LLM_PROVIDER",
            "value": "groq"
          }
        ],
        "secrets": [
          {
            "name": "GROQ_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-2:339712993834:secret:apikeyliveclassTest-q1PGdB:GROQ_API_KEY::"
          },
          {
            "name": "GOOGLE_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-2:339712993834:secret:apikeyliveclassTest-q1PGdB:GOOGLE_API_KEY::"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/llmopstdlive",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs",
            "awslogs-create-group": "true"
          }
        }
      }
    ]
  }
